msgid "VTEI aanvragen"
msgstr ""

#: astrid/templates/astrid/vtei_request.html:10
msgid ""
"\n"
"    VTEI is een aanvraag om het abonnement van een radio over te zetten op "
"een nieuwe radio met behoud van het ISSI op de nieuwe radio.\n"
"    "
msgstr ""
"\n"
"    VTEI est une demande pour transférer l'abonnement d'une radio vers une "
"nouvelle radio tout en conservant l'ISSI sur la nouvelle radio.\n"
"    "

#: radio/forms.py:16
msgid "TEI must contain only digits."
msgstr "Le TEI ne doit contenir que des chiffres."

#: radio/forms.py:19
msgid "TEI must be 14 or 15 digits long."
msgstr "Le TEI doit comporter 14 ou 15 chiffres."

#: radio/forms.py:24
msgid "If 15 digits, last digit must be zero."
msgstr "Si le TEI comporte 15 chiffres, le dernier doit être zéro."

#: radio/forms.py:31
msgid "TEI is not within known TEI ranges."
msgstr "Le TEI ne se trouve pas dans les plages TEI connues."

